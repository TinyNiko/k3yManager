import os
import sys 
import subprocess

allpath = []

def getfile(path) :
	for root ,dirname ,filename in os.walk(path):
		for f in filename:
			if root=='D:\\down':	
				print f.split('.')[0] 
def  main():
	global allpath 
	for path in allpath :
		getfile(path)
if __name__ == '__main__':
	if len(sys.argv) >1 :
		allpath.append(sys.argv[1]) 
	main()

